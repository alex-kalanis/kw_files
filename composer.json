{
    "name": "alex-kalanis/kw_files",
    "description": "Accessing stored entries as files on some volume",
    "keywords": [
        "files",
        "file",
        "volume"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "alex-kalanis/kw_paths": ">=4.0 <5",
        "alex-kalanis/kw_storage": ">=5.0 <6",
        "ext-mbstring": "*",
        "php": ">=7.4.0"
    },
    "require-dev": {
        "shipmonk/composer-dependency-analyser": "^1.4",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": ">=8 <=9"
    },
    "suggest": {
        "alex-kalanis/kw_storage": "Local storage for accessing resources"
    },
    "autoload": {
        "psr-4": {
            "kalanis\\kw_files\\": "php-src/"
        }
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
